package com.secondhighest.array;

public class FindLatestValueInArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int arr[] = { 12, 5, 0, 44, 8 };
		int max = 0;
		for (int i = 0; i < arr.length; i++) {
			if (arr[i] > max) {
				max = arr[i];
			}
		}
		System.out.println("Max value inside array:" + max);
	}

}
